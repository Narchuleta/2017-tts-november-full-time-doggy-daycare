<%= form_for @dog do |form_field| %>
  <%= form_field.label :name %>
  <%= form_field.text_field :name %>
  <br>

  <%= form_field.label :breed_id %>
  <%= form_field.collection_select :breed_id, Breed.all, :id, :name %>
  <br>

  <%= form_field.label :age %>
  <%= form_field.number_field :age %>
  <br>

  <%= form_field.label :owner_id %>
  <%= form_field.collection_select :owner_id, Owner.all, :id, :name %>
  <br>

  <%= form_field.submit %>
<% end %>